40 POKE &HFFD9,0
50 ONBRK GOTO 210
60 POKE &H3000,&H80
70 POKE &H3001,&H00
80 EXEC &H3008
90 D$=" FCB "
100 FOR I=1 TO PEEK(&H3002)
110 D$=D$+HEX$(PEEK(&H3004+I-1))+","
120 NEXT I
130 PRINT D$
140 POKE &HFFD8,0
150 PRINT #1,D$
160 POKE &HFFD9,0
170 L=L+PEEK(&H3002)
